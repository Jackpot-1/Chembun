config_version=5

[application]

config/name="Chembun"
run/main_scene="res://assets/Scenes/Screens/StartingScreen.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GGS="*res://addons/ggs/classes/global/ggs.tscn"
Globals="*res://assets/Scripts/autoloads/Globals.gd"

[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/handheld/orientation=4

[editor_plugins]

enabled=PackedStringArray("res://addons/ThirdPersonTemplate/plugin.cfg", "res://addons/ggs/plugin.cfg", "res://addons/gloot/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/": "red",
"res://assets/fuk/": "yellow",
"res://game_settings/": "yellow"
}

